<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HRAS</title>
    <link rel="stylesheet" href="~/lib/rewire.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/stylus/dist/new.css" asp-append-version="true" />
</head>
<body>
    <header>
        <partial name="_navbar.cshtml" />
    </header>
    <div class="container-fluid">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer class="privacy">
        <div class="container">
            &copy; 2023 - HRAS - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

